{
  "name": "06-api-express-typescript",
  "version": "0.2.6",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run parcel:build && node src/index.js",
    "dev": "npm run parcel:dev & npm run nodemon",
    "nodemon": "nodemon src/index.js",
    "parcel:build": "rm -rf ./public; parcel build src/view/index.html --out-dir public --no-source-maps",
    "parcel:dev": "rm -rf ./public; parcel --port 9010 src/view/index.html --out-dir public"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 12
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.0.25",
    "@types/mongoose": "^5.7.14",
    "babel-jest": "^24.9.0",
    "better-docs": "^1.4.7",
    "eslint": "^6.8.0",
    "husky": "^4.2.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "minami": "^1.2.3",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "replace": "^1.1.5",
    "standard-version": "^7.1.0",
    "tslint": "^6.1.1"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.1.0",
    "axios": "^0.19.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.17",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.1.2",
    "sweetalert": "^2.1.2"
  }
}
